{
  "name": "cel",
  "version": "0.1.2",
  "description": "A javascript component micro-framework",
  "main": "index.js",
  "scripts": {
    "build:dev": "node node_modules/rollup/bin/rollup -c ./build/dev-config.js",
    "build:dev:watch": "node node_modules/rollup/bin/rollup -c ./build/dev-config.js -w",
    "build:prod": "node node_modules/rollup/bin/rollup -c ./build/prod-config.js",
    "build:all": "npm run build:dev && npm run build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leocreatini/cel.git"
  },
  "keywords": [
    "component",
    "framework"
  ],
  "author": "Leo Creatini",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leocreatini/cel/issues"
  },
  "homepage": "https://github.com/leocreatini/cel#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "rollup": "^0.38.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^2.5.0"
  }
}
